{
  "name": "yurineko-microservice-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "proto:install": "yarn add --dev git+ssh://git@github.com:Yurineko-Service/yurineko-microservice-proto.git#main",
    "proto:taxonomy": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.bin -I=./node_modules/yurineko-microservice-proto/proto --ts_proto_out=src/taxonomy/ node_modules/yurineko-microservice-proto/proto/taxonomy.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto": "yarn proto:install && yarn proto:taxonomy"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@grpc/grpc-js": "^1.8.0",
    "@grpc/proto-loader": "^0.7.4",
    "@nestjs/apollo": "^11.0.4",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.3.12",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@prisma/client": "^4.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.7",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.15",
    "@types/jest": "29.2.4",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.3.1",
    "prettier": "^2.8.1",
    "prisma": "^4.11.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.136.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.4",
    "yurineko-microservice-proto": "git+ssh://git@github.com:Yurineko-Service/yurineko-microservice-proto.git#main"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
