{
  "name": "yurineko-api-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "proto:install": "yarn add --dev git+ssh://git@github.com:Yurineko-Service/yurineko-microservice-proto.git#main",
    "proto:auth": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.bin -I=./node_modules/yurineko-microservice-proto/proto --ts_proto_out=src/auth/ node_modules/yurineko-microservice-proto/proto/auth.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:user": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.bin -I=./node_modules/yurineko-microservice-proto/proto --ts_proto_out=src/user/ node_modules/yurineko-microservice-proto/proto/user.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:notification": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.bin -I=./node_modules/yurineko-microservice-proto/proto --ts_proto_out=src/notification/ node_modules/yurineko-microservice-proto/proto/notification.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:user-preference": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.bin -I=./node_modules/yurineko-microservice-proto/proto --ts_proto_out=src/user-preference/ node_modules/yurineko-microservice-proto/proto/user-preference.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:storage": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.bin -I=./node_modules/yurineko-microservice-proto/proto --ts_proto_out=src/storage/ node_modules/yurineko-microservice-proto/proto/storage.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:push": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.bin -I=./node_modules/yurineko-microservice-proto/proto --ts_proto_out=src/push/ node_modules/yurineko-microservice-proto/proto/push.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto": "yarn proto:install && yarn proto:auth && yarn proto:user && yarn proto:notification && yarn proto:user-preference && yarn proto:storage && yarn proto:push"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/swagger": "^6.1.3",
    "helmet": "^6.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "29.2.3",
    "@types/node": "^18.11.9",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.3.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.132.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.9.3",
    "yurineko-microservice-proto": "git+ssh://git@github.com:Yurineko-Service/yurineko-microservice-proto.git#main"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
